{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "~2.0.0@stable",
        "silverstripe/vendor-plugin": "~2.0.2@stable",
        "silverstripe/recipe-cms": "~5.2.0@stable",
        "silverstripe-themes/simple": "~3.3.1@stable",
        "silverstripe/login-forms": "~5.2.0@stable",
        "tractorcow/silverstripe-fluent": "^7.1",
        "silverstripe/userforms": "^6.2",
        "silverstripe/spamprotection": "^4.2",
        "wilr/silverstripe-googlesitemaps": "^3.1",
        "silverstripe/siteconfig": "^5.2",
        "silverstripe/reports": "^5.2",
        "symfony/mailer": "^6.4",
        "cwp/starter-theme": "^4.1",
        "loveduckie/silverstripe-json-ld-structured-data": "4.x-dev",
        "undefinedoffset/silverstripe-nocaptcha": "^2.4",
        "cwp/watea-theme": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "extra": {
        "expose": [
            "themes/simple/css",
            "themes/simple/javascript",
            "themes/simple/images",
            "themes/simple/webfonts",
            "themes/starter/dist",
            "themes/starter/ico",
            "themes/starter/images",
            "themes/birdwatcherconf/css",
            "themes/birdwatcherconf/ico"
        ],
        "project-files-installed": [
            ".htaccess",
            "app/.htaccess",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
